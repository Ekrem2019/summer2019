package day19_ForLoop;

public class ForLoop {
	
	public static void main(String[] args) {
		
		System.out.println("   ^");
        System.out.println("  /|\\");
        System.out.println(" / | \\");
        System.out.println("/  |  \\");
        
         for (int i = 0 ; i <3; i++)
         {
         
        System.out.println("|  |  |");
        
         }
        System.out.println("-------");
        
        for (int e=2500; e>0; e-=5) {
        	System.out.println("Hello"+e);
        }
        for (int a=1; a<=50; a++) {
        	System.out.println("the square of "+a+" is: "+(a*a));
        }
        for (int q=1; q<=32; q++) {
        	System.out.println(q);
        }
        
        
        
        
        
        
        
}
}