package String_Study;

import java.util.Scanner;

public class checkwords {
	public static void main(String[] args) {
		
		Scanner input = new Scanner (System.in);
		System.out.println("Enter 3 words");
		String w1=input.next();
		String w2=input.next();
		String w3=input.next();
		// int n = name.length()  ; 
		int v1 = w1.length();
		int v2 = w2.length();
		int v3 = w3.length();
		
	if (v1==v2 && v2==v3) {System.out.println("All words are same length");
		
	} else if (v1>v2 || v2==v3) {System.out.println("Not Same nor Different lengths");}
	else if (v2>v3 || v3==v1) {System.out.println("Not Same nor Different lengths");}
	else if (v3>v1 || v1==v2) {System.out.println("Not Same nor Different lengths");}
	else {System.out.println("All different length");}
		
		
		

		
		
		
		
		
	}

}
