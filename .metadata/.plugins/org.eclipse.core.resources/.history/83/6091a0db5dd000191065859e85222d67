package day15_Scanner_StringClass;

import java.util.*;

public class test {
	
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter info");
		String txt = s.next();
		System.out.print(txt.charAt(2)+""+txt.charAt(4)+""+txt.charAt(5) );
		
		
	}

}
