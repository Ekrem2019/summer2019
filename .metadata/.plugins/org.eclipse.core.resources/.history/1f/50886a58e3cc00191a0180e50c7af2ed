package day13_Review;

public class SwitchStatement {
	
	public static void main(String[] args) {
		
		int num =7;
		switch (num) {
		case 5:
			System.out.println("sunday");
			break;
			
			default: 
				System.out.println("invalid entry");
				break;
			
			case 10-3:
				System.out.println("friday");
				break;			
		
		}
	
	String days="monday";
	switch (days) {
			
	case "birday":
		System.out.println("fun day");
		break;
		
	case "tuesday":
		System.out.println("tuesday is great day");
	break;

	default:
		System.out.println("invalid");
	
	case "wednesday":
		System.out.println("wednesday is of day");
		break;
		
		
	
}
	String us="usa";
	switch (us) {
	case "us":
	case"usa":
	case"american":
		System.out.println("american");
	break;
	default:
		System.out.println("not american");
	}
		
	    String p1 = "James";
	    String p2 = "Jill";
	    String p3 = "Bob";
	    String p4 = "Betty";
	    String p5 = "Herb";
	    
	    System.out.println(p3+","+p2+","+p4+","+p5+","+p1);

		
		
	}
		
		
	
	
	}


