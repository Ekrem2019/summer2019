package day15_Scanner_StringClass;

import java.util.Scanner;

public class Task2 {
	
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Enter Three Number");
		int intNum=input.nextByte();
		int intNum2=input.nextByte();
		int intNum3=input.nextByte();
		
		if (intNum>intNum2 && intNum>intNum3) {
			System.out.println("Largest number is : "+intNum);
		}else if(intNum2>intNum && intNum2>intNum3) {
			System.out.println("Largest number is : "+intNum2);
		}else {System.out.println("Largest number is : "+intNum3);
		
		}
					
	
	
	

	}

}
