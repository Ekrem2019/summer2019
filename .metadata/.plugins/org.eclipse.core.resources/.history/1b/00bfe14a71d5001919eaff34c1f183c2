package day19_ForLoop;

public class ForLoop {
	
	public static void main(String[] args) {
		
		System.out.println("   ^");
        System.out.println("  /|\\");
        System.out.println(" / | \\");
        System.out.println("/  |  \\");
        
         for (int i = 0 ; i < 8; i++)
         {
         
        System.out.println("|  |  |");
        
         }
        System.out.println("-------");

}
}