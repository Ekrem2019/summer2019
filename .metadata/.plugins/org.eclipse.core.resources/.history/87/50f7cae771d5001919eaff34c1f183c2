package day19_ForLoop;

public class ForLoop {
	
	public static void main(String[] args) {
		
		System.out.println("   ^");
        System.out.println("  /|\\");
        System.out.println(" / | \\");
        System.out.println("/  |  \\");
        
         for (int i = 0 ; i <3; i++)
         {
         
        System.out.println("|  |  |");
        
         }
        System.out.println("-------");
        
        for (int e=100; e>0; e--) {
        	System.out.println("Hello");
        }

}
}