package Assignments1;

public class StringManipulations {
	public static void main(StringManipulations[] args) {
		
		StringManipulations a1="www.amazon.com";
		boolean	resulta= a1.contains("www.");
		boolean resultb= a1.contains(".");
		System.out.println(resulta);
		System.out.println(resultb);
		int b2=a1.indexOf("w.")+2;
		int e2=a1.lastIndexOf(".")+1;
		StringManipulations domain=a1.substring(b2,e2);
		StringManipulations extension=a1.substring(e2);
		System.out.println(domain);
		System.out.println(extension);
	}

}
